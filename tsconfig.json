{
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",

    "paths": {
      "@/*": [
        "./*"
      ],
      "@/components/*": [
        "components/*"
      ],
      "@/assets/*": [
        "assets/*"
      ]
    },
   
    "jsx": "react-native",
    "moduleResolution": "node",
    "target": "esnext",
    "allowJs": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "custom.d.ts",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
, "app/(public)/(onboarding)/savety.tsxxxxx"  ],
  "extends": "expo/tsconfig.base"
}
