{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ],
      "@/components/*": ["./components/*"],
      "@/assets/*": ["./assets/*"]
    },
    "typeRoots": ["./node_modules/@types", "./custom.d.ts"]
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "custom.d.ts"
  ]
}